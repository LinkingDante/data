<https://dantenetwork.it/ontology/syntit/current/> rdf:type owl:Ontology ;
                                                    <http://purl.org/dc/elements/1.1/creator> "Carlo Meghini, Daniele Metilli, Cesare Concordia, Gaia Tomazzoli" ;
                                                    <http://purl.org/dc/elements/1.1/date> "2022-12-21" ;
                                                    <http://purl.org/dc/elements/1.1/publisher> "ISTI-CNR" ;
                                                    rdfs:comment "Ontology for representing the syntax of the Italian vernacular of the 14th century. Added CRIA for clause types"^^xsd:string ;
