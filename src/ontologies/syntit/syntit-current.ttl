#################################################################
#    Object Properties
#################################################################

###  http://erlangen-crm.org/current/P127_has_broader_term
ecrm:P127_has_broader_term rdf:type owl:ObjectProperty ;
                           rdfs:label "P127 has broader term" .


###  http://erlangen-crm.org/current/P148_has_component
ecrm:P148_has_component rdf:type owl:ObjectProperty ;
                        rdfs:subPropertyOf owl:topObjectProperty ;
                        owl:inverseOf ecrm:P148i_is_component_of ;
                        rdfs:label "P148 has component"@en .


###  http://erlangen-crm.org/current/P148i_is_component_of
ecrm:P148i_is_component_of rdf:type owl:ObjectProperty ;
                           rdfs:label "P148i is component of" .


###  http://erlangen-crm.org/current/P2_has_type
ecrm:P2_has_type rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf owl:topObjectProperty ;
                 owl:inverseOf ecrm:P2i_is_type_of ;
                 rdfs:label "P2 has type"@en .


###  http://erlangen-crm.org/current/P2i_is_type_of
ecrm:P2i_is_type_of rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf owl:topObjectProperty ;
                    rdfs:label "P2i is type of" .


###  https://dantenetwork.it/ontology/olires/current/occurrenceOf
olires:occurrenceOf rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf olires:oliresTopObjectProperty ;
                    rdfs:domain olires:TextFragment ;
                    rdfs:range olires:SyntacticEntity .


###  https://dantenetwork.it/ontology/olires/current/oliresTopObjectProperty
olires:oliresTopObjectProperty rdf:type owl:ObjectProperty ;
                               rdfs:label "OLiRes top object property" .


###  https://dantenetwork.it/ontology/syntit/current/hasClauseFunction
syntit:hasClauseFunction rdf:type owl:ObjectProperty ;
                         rdfs:subPropertyOf ecrm:P2_has_type ;
                         owl:inverseOf syntit:isClauseFunctionOf ;
                         rdfs:domain syntit:ClauseOccurrence ;
                         rdfs:range syntit:ClauseFunction ;
                         rdfs:label "ha funzione della frase"@it ,
                                    "has clause function"@en .


###  https://dantenetwork.it/ontology/syntit/current/hasClauseType
syntit:hasClauseType rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf ecrm:P2_has_type ;
                     owl:inverseOf syntit:isClauseTypeOf ;
                     rdfs:domain syntit:ClauseOccurrence ;
                     rdfs:range [ rdf:type owl:Class ;
                                  owl:unionOf ( syntit:ClauseSubType
                                                syntit:ClauseType
                                              )
                                ] ;
                     owl:propertyChainAxiom ( syntit:hasClauseType
                                              ecrm:P127_has_broader_term
                                            ) ;
                     rdfs:label "ha tipo della frase"@it ,
                                "has clause type"@en .


###  https://dantenetwork.it/ontology/syntit/current/hasComponentSyntagm
syntit:hasComponentSyntagm rdf:type owl:ObjectProperty ;
                           rdfs:subPropertyOf syntit:hasSyntacticalComponent ;
                           rdf:type owl:AsymmetricProperty ,
                                    owl:IrreflexiveProperty ;
                           rdfs:domain syntit:Syntagm ;
                           rdfs:range syntit:Syntagm ;
                           rdfs:label "has component syntagm" .


###  https://dantenetwork.it/ontology/syntit/current/hasCoordinate
syntit:hasCoordinate rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf olires:oliresTopObjectProperty ;
                     owl:inverseOf syntit:isCoordinateOf ;
                     rdf:type owl:SymmetricProperty ,
                              owl:IrreflexiveProperty ;
                     rdfs:domain syntit:ClauseOccurrence ;
                     rdfs:range syntit:ClauseOccurrence ;
                     rdfs:label "ha coordinata"@it ,
                                "has coordinate"@en .


###  https://dantenetwork.it/ontology/syntit/current/hasDiscourseType
syntit:hasDiscourseType rdf:type owl:ObjectProperty ;
                        rdfs:subPropertyOf ecrm:P2_has_type ;
                        owl:inverseOf syntit:isDiscourseTypeOf ;
                        rdfs:domain syntit:Discourse ;
                        rdfs:range syntit:DiscourseType ;
                        rdfs:label "ha tipo del discorso"@it ,
                                   "has discourse type"@en .


###  https://dantenetwork.it/ontology/syntit/current/hasHead
syntit:hasHead rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf syntit:hasSyntacticalComponent ;
               rdfs:label "has head" .


###  https://dantenetwork.it/ontology/syntit/current/hasModifier
syntit:hasModifier rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf syntit:hasSyntacticalComponent ;
                   rdfs:domain syntit:Syntagm ;
                   rdfs:range ecrm:E33_Linguistic_Object ;
                   rdfs:label "has modifier" .


###  https://dantenetwork.it/ontology/syntit/current/hasSpeaker
syntit:hasSpeaker rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf olires:oliresTopObjectProperty ;
                  owl:inverseOf syntit:isSpeakerOf ;
                  rdfs:domain syntit:Discourse ;
                  rdfs:range ecrm:E39_Actor ;
                  rdfs:label "has speaker"@en .


###  https://dantenetwork.it/ontology/syntit/current/hasSubordinate
syntit:hasSubordinate rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf olires:oliresTopObjectProperty ;
                      owl:inverseOf syntit:isSubordinateOf ;
                      rdf:type owl:InverseFunctionalProperty ,
                               owl:AsymmetricProperty ,
                               owl:IrreflexiveProperty ;
                      rdfs:domain syntit:ClauseOccurrence ;
                      rdfs:range syntit:ClauseOccurrence ;
                      rdfs:label "ha subordinata"@it ,
                                 "has subordinate"@en .


###  https://dantenetwork.it/ontology/syntit/current/hasSyntacticalComponent
syntit:hasSyntacticalComponent rdf:type owl:ObjectProperty ;
                               rdfs:subPropertyOf ecrm:P148_has_component .


###  https://dantenetwork.it/ontology/syntit/current/includesClause
syntit:includesClause rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf syntit:hasSyntacticalComponent ;
                      rdfs:domain syntit:Syntagm ;
                      rdfs:range syntit:Clause ;
                      rdfs:label "includes clause" .


###  https://dantenetwork.it/ontology/syntit/current/isClauseFunctionOf
syntit:isClauseFunctionOf rdf:type owl:ObjectProperty ;
                          rdfs:subPropertyOf ecrm:P2i_is_type_of ;
                          rdfs:label "is clause function of"@en ,
                                     "è funzione della frase"@it .


###  https://dantenetwork.it/ontology/syntit/current/isClauseTypeOf
syntit:isClauseTypeOf rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf ecrm:P2i_is_type_of ;
                      rdfs:label "is clause type of"@en ,
                                 "è tipo della frase"@it .


###  https://dantenetwork.it/ontology/syntit/current/isCoordinateOf
syntit:isCoordinateOf rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf olires:oliresTopObjectProperty ;
                      rdfs:label "is coordinate of"@en ,
                                 "è coordinata a"@it .


###  https://dantenetwork.it/ontology/syntit/current/isDiscourseTypeOf
syntit:isDiscourseTypeOf rdf:type owl:ObjectProperty ;
                         rdfs:subPropertyOf ecrm:P2i_is_type_of ;
                         rdfs:label "is discourse type of"@en ,
                                    "è tipo del discorso"@it .


###  https://dantenetwork.it/ontology/syntit/current/isSpeakerOf
syntit:isSpeakerOf rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf olires:oliresTopObjectProperty ;
                   rdfs:label "is speaker of"@en .


###  https://dantenetwork.it/ontology/syntit/current/isSubordinateOf
syntit:isSubordinateOf rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf olires:oliresTopObjectProperty ;
                       rdf:type owl:FunctionalProperty ;
                       rdfs:label "is subordinate of"@en ,
                                  "è subordinata a"@it .


#################################################################
#    Classes
#################################################################

###  http://erlangen-crm.org/current/E33_Linguistic_Object
ecrm:E33_Linguistic_Object rdf:type owl:Class ;
                           rdfs:subClassOf [ rdf:type owl:Restriction ;
                                             owl:onProperty [ owl:inverseOf syntit:hasModifier
                                                            ] ;
                                             owl:allValuesFrom syntit:Syntagm
                                           ] ;
                           rdfs:label "E33 Linguistic Object"@en .


###  http://erlangen-crm.org/current/E39_Actor
ecrm:E39_Actor rdf:type owl:Class ;
               rdfs:subClassOf [ rdf:type owl:Restriction ;
                                 owl:onProperty syntit:isSpeakerOf ;
                                 owl:allValuesFrom syntit:Discourse
                               ] ;
               rdfs:label "E39 Actor" .


###  http://erlangen-crm.org/current/E55_Type
ecrm:E55_Type rdf:type owl:Class ;
              rdfs:subClassOf owl:Thing ;
              rdfs:label "E55 Type" .


###  https://dantenetwork.it/ontology/olires/current/SyntacticEntity
olires:SyntacticEntity rdf:type owl:Class ;
                       rdfs:subClassOf ecrm:E33_Linguistic_Object ;
                       rdfs:label "Syntactic Entity" .


###  https://dantenetwork.it/ontology/olires/current/TextFragment
olires:TextFragment rdf:type owl:Class ;
                    rdfs:subClassOf ecrm:E33_Linguistic_Object ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty [ owl:inverseOf syntit:hasModifier
                                                     ] ;
                                      owl:allValuesFrom syntit:SyntagmOccurrence
                                    ] ;
                    rdfs:label "Text Fragment" .


###  https://dantenetwork.it/ontology/orl/current/FormOccurrence
orl:FormOccurrence rdf:type owl:Class ;
                   rdfs:subClassOf olires:TextFragment ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty [ owl:inverseOf syntit:hasHead
                                                    ] ;
                                     owl:allValuesFrom syntit:SyntagmOccurrence
                                   ] .


###  https://dantenetwork.it/ontology/orl/current/FormaAggettivoVolgare
orl:FormaAggettivoVolgare rdf:type owl:Class ;
                          rdfs:subClassOf orl:FormaItalianoVolgare ,
                                          [ rdf:type owl:Restriction ;
                                            owl:onProperty [ owl:inverseOf syntit:hasHead
                                                           ] ;
                                            owl:allValuesFrom syntit:AdjectivePhrase
                                          ] .


###  https://dantenetwork.it/ontology/orl/current/FormaItalianoVolgare
orl:FormaItalianoVolgare rdf:type owl:Class ;
                         rdfs:subClassOf [ rdf:type owl:Restriction ;
                                           owl:onProperty [ owl:inverseOf syntit:hasHead
                                                          ] ;
                                           owl:allValuesFrom syntit:Syntagm
                                         ] .


###  https://dantenetwork.it/ontology/orl/current/FormaPreposizioneVolgare
orl:FormaPreposizioneVolgare rdf:type owl:Class ;
                             rdfs:subClassOf orl:FormaItalianoVolgare ,
                                             [ rdf:type owl:Restriction ;
                                               owl:onProperty [ owl:inverseOf syntit:hasHead
                                                              ] ;
                                               owl:allValuesFrom syntit:PrepositionalPhrase
                                             ] .


###  https://dantenetwork.it/ontology/orl/current/FormaSostantivoVolgare
orl:FormaSostantivoVolgare rdf:type owl:Class ;
                           rdfs:subClassOf orl:FormaItalianoVolgare ,
                                           [ rdf:type owl:Restriction ;
                                             owl:onProperty [ owl:inverseOf syntit:hasHead
                                                            ] ;
                                             owl:allValuesFrom syntit:NounPhrase
                                           ] .


###  https://dantenetwork.it/ontology/orl/current/FormaVerboVolgare
orl:FormaVerboVolgare rdf:type owl:Class ;
                      rdfs:subClassOf orl:FormaItalianoVolgare ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty [ owl:inverseOf syntit:hasHead
                                                       ] ;
                                        owl:allValuesFrom syntit:VerbPhrase
                                      ] .


###  https://dantenetwork.it/ontology/syntit/current/AdjectivePhrase
syntit:AdjectivePhrase rdf:type owl:Class ;
                       rdfs:subClassOf syntit:Syntagm ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty syntit:hasHead ;
                                         owl:allValuesFrom orl:FormaAggettivoVolgare
                                       ] ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty [ owl:inverseOf olires:occurrenceOf
                                                        ] ;
                                         owl:allValuesFrom syntit:AdjectivePhraseOccurrence
                                       ] ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty syntit:hasHead ;
                                         owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                         owl:onClass orl:FormaAggettivoVolgare
                                       ] ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty [ owl:inverseOf olires:occurrenceOf
                                                        ] ;
                                         owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                         owl:onClass syntit:AdjectivePhraseOccurrence
                                       ] .


###  https://dantenetwork.it/ontology/syntit/current/AdjectivePhraseOccurrence
syntit:AdjectivePhraseOccurrence rdf:type owl:Class ;
                                 rdfs:subClassOf syntit:SyntagmOccurrence ,
                                                 [ rdf:type owl:Restriction ;
                                                   owl:onProperty olires:occurrenceOf ;
                                                   owl:allValuesFrom syntit:AdjectivePhrase
                                                 ] ,
                                                 [ rdf:type owl:Restriction ;
                                                   owl:onProperty olires:occurrenceOf ;
                                                   owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                   owl:onClass syntit:AdjectivePhrase
                                                 ] .


###  https://dantenetwork.it/ontology/syntit/current/Clause
syntit:Clause rdf:type owl:Class ;
              rdfs:subClassOf olires:SyntacticEntity ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty syntit:hasSyntacticalComponent ;
                                owl:allValuesFrom syntit:Syntagm
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty [ owl:inverseOf olires:occurrenceOf
                                               ] ;
                                owl:allValuesFrom syntit:ClauseOccurrence
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty [ owl:inverseOf syntit:hasSyntacticalComponent
                                               ] ;
                                owl:allValuesFrom syntit:Sentence
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty [ owl:inverseOf syntit:includesClause
                                               ] ;
                                owl:allValuesFrom syntit:Syntagm
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty syntit:hasSyntacticalComponent ;
                                owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                owl:onClass syntit:Syntagm
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty [ owl:inverseOf olires:occurrenceOf
                                               ] ;
                                owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                owl:onClass syntit:ClauseOccurrence
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty [ owl:inverseOf syntit:hasSyntacticalComponent
                                               ] ;
                                owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                owl:onClass syntit:Sentence
                              ] ;
              rdfs:label "Clause"@en ,
                         "Frase"@it .


###  https://dantenetwork.it/ontology/syntit/current/ClauseFunction
syntit:ClauseFunction rdf:type owl:Class ;
                      rdfs:subClassOf ecrm:E55_Type ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty syntit:isClauseFunctionOf ;
                                        owl:someValuesFrom syntit:Clause
                                      ] ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty syntit:isClauseFunctionOf ;
                                        owl:allValuesFrom syntit:Clause
                                      ] ;
                      rdfs:label "Clause Function"@en ,
                                 "Funzione della frase"@it .


###  https://dantenetwork.it/ontology/syntit/current/ClauseOccurrence
syntit:ClauseOccurrence rdf:type owl:Class ;
                        rdfs:subClassOf olires:TextFragment ,
                                        [ rdf:type owl:Restriction ;
                                          owl:onProperty olires:occurrenceOf ;
                                          owl:allValuesFrom syntit:Clause
                                        ] ,
                                        [ rdf:type owl:Restriction ;
                                          owl:onProperty syntit:hasSyntacticalComponent ;
                                          owl:allValuesFrom syntit:SyntagmOccurrence
                                        ] ,
                                        [ rdf:type owl:Restriction ;
                                          owl:onProperty [ owl:inverseOf syntit:hasSyntacticalComponent
                                                         ] ;
                                          owl:allValuesFrom syntit:SentenceOccurrence
                                        ] ,
                                        [ rdf:type owl:Restriction ;
                                          owl:onProperty [ owl:inverseOf syntit:includesClause
                                                         ] ;
                                          owl:allValuesFrom syntit:SyntagmOccurrence
                                        ] ,
                                        [ rdf:type owl:Restriction ;
                                          owl:onProperty syntit:hasSyntacticalComponent ;
                                          owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                          owl:onClass syntit:SyntagmOccurrence
                                        ] ,
                                        [ rdf:type owl:Restriction ;
                                          owl:onProperty olires:occurrenceOf ;
                                          owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                          owl:onClass syntit:Clause
                                        ] ,
                                        [ rdf:type owl:Restriction ;
                                          owl:onProperty [ owl:inverseOf syntit:hasSyntacticalComponent
                                                         ] ;
                                          owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                          owl:onClass syntit:SentenceOccurrence
                                        ] ;
                        rdfs:label "Clause Occurrence" .


###  https://dantenetwork.it/ontology/syntit/current/ClauseSubType
syntit:ClauseSubType rdf:type owl:Class ;
                     rdfs:subClassOf ecrm:E55_Type ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty syntit:isClauseTypeOf ;
                                       owl:someValuesFrom syntit:Clause
                                     ] ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty syntit:isClauseTypeOf ;
                                       owl:allValuesFrom syntit:Clause
                                     ] ;
                     rdfs:label "Clause Sub Type"@en ,
                                "Sottotipo della frase"@it .


###  https://dantenetwork.it/ontology/syntit/current/ClauseType
syntit:ClauseType rdf:type owl:Class ;
                  rdfs:subClassOf ecrm:E55_Type ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty syntit:isClauseTypeOf ;
                                    owl:allValuesFrom syntit:Clause
                                  ] ;
                  rdfs:label "Clause Type"@en ,
                             "Tipo della frase"@it .


###  https://dantenetwork.it/ontology/syntit/current/Discourse
syntit:Discourse rdf:type owl:Class ;
                 rdfs:subClassOf olires:TextFragment ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty syntit:hasSpeaker ;
                                   owl:someValuesFrom ecrm:E39_Actor
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty syntit:hasSpeaker ;
                                   owl:allValuesFrom ecrm:E39_Actor
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty syntit:hasSyntacticalComponent ;
                                   owl:allValuesFrom syntit:SentenceOccurrence
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty syntit:hasSyntacticalComponent ;
                                   owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                   owl:onClass syntit:SentenceOccurrence
                                 ] ;
                 rdfs:label "Discorso"@it ,
                            "Discourse"@en .


###  https://dantenetwork.it/ontology/syntit/current/DiscourseType
syntit:DiscourseType rdf:type owl:Class ;
                     rdfs:subClassOf ecrm:E55_Type ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty syntit:isDiscourseTypeOf ;
                                       owl:someValuesFrom syntit:Discourse
                                     ] ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty syntit:isDiscourseTypeOf ;
                                       owl:allValuesFrom syntit:Discourse
                                     ] ;
                     rdfs:label "Discourse Type"@en ,
                                "Tipo del discorso"@it .


###  https://dantenetwork.it/ontology/syntit/current/NounPhrase
syntit:NounPhrase rdf:type owl:Class ;
                  rdfs:subClassOf syntit:Syntagm ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty syntit:hasHead ;
                                    owl:allValuesFrom orl:FormaSostantivoVolgare
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty [ owl:inverseOf olires:occurrenceOf
                                                   ] ;
                                    owl:allValuesFrom syntit:NounPhraseOccurrence
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty syntit:hasHead ;
                                    owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                    owl:onClass orl:FormaSostantivoVolgare
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty [ owl:inverseOf olires:occurrenceOf
                                                   ] ;
                                    owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                    owl:onClass syntit:NounPhraseOccurrence
                                  ] .


###  https://dantenetwork.it/ontology/syntit/current/NounPhraseOccurrence
syntit:NounPhraseOccurrence rdf:type owl:Class ;
                            rdfs:subClassOf syntit:SyntagmOccurrence ,
                                            [ rdf:type owl:Restriction ;
                                              owl:onProperty olires:occurrenceOf ;
                                              owl:allValuesFrom syntit:NounPhrase
                                            ] ,
                                            [ rdf:type owl:Restriction ;
                                              owl:onProperty olires:occurrenceOf ;
                                              owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                              owl:onClass syntit:NounPhrase
                                            ] .


###  https://dantenetwork.it/ontology/syntit/current/PrepositionalPhrase
syntit:PrepositionalPhrase rdf:type owl:Class ;
                           rdfs:subClassOf syntit:Syntagm ,
                                           [ rdf:type owl:Restriction ;
                                             owl:onProperty syntit:hasHead ;
                                             owl:allValuesFrom orl:FormaPreposizioneVolgare
                                           ] ,
                                           [ rdf:type owl:Restriction ;
                                             owl:onProperty [ owl:inverseOf olires:occurrenceOf
                                                            ] ;
                                             owl:allValuesFrom syntit:PrepositionalPhraseOccurrence
                                           ] ,
                                           [ rdf:type owl:Restriction ;
                                             owl:onProperty syntit:hasHead ;
                                             owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                             owl:onClass orl:FormaPreposizioneVolgare
                                           ] ,
                                           [ rdf:type owl:Restriction ;
